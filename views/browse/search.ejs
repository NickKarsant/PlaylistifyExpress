<% layout('layouts/boilerplate') %>

<div class="row">
  <%- include('../partials/aside') %>



  <div class="col-xs-10 col-sm-10 col-md-9 col-lg-10">
    <form action="/search" method="GET" style="width:50%; margin-bottom: 1em;">
      <h2>Search</h2>
      <div class="input-group p-1" >
        <input autofocus type="search" placeholder="Search for artists or songs" aria-describedby="button-addon3"
          class="form-control bg-none border-1" >
      </div>
    </form>
    
    <div id="results">
      <div id="artistsSearch">
        <h2>Artists</h2>

      </div>


      <div id="playlistsSearch">
        <h2>Playlists</h2>

      </div>


      <div id="songsSearch">
        <h2>Songs</h2>

      </div>

    </div>
  
  
  
  
  
  
  <div>
</div>