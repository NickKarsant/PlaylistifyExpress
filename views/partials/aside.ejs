<div class="col-xs-2 col-sm-2 col-md-3 col-lg-2" style="padding: 0; width:auto;height:auto; border: 1px solid red;">
  <div>
    <ul class="aside">
      <% if(currentUser) {%>
      <li>
        <a id="home" class="navigation" href="/playlists"><span><i class="fas fa-home"></i></span>Home</a>
      </li>
      <li>
        <a id="browse" styl="margin-top:1em;" class="navigation" href="/browse"><span><i
              class="fas fa-eye"></i></span>Browse</a>
      </li>
      <%} else {%>
      <li>
        <a id="browse" class="navigation" href="/browse"><span><i class="fas fa-eye"></i></span>Browse</a>
      </li>
      <%}%>
        </li>
        <li>
            <a id="browse" class="navigation" href="/search"><span><i class="fas fa-search"></i></span>Search</a>

        </li>
      </ul>

      <ul class="aside pt-3">
        <h4 id="list">PLAYLISTS</h4>
        <% if(!currentUser) {%>
      <li>
        <a class="btn btn-sm " href="/playlists/new"><i id="sideAdd" class="far fa-plus-square">Create Playlist</i> </a>
      </li>
      <%} else {%>
      <li>
        <a class="btn btn-sm " href="/playlists/new"><i id="sideAdd" class="far fa-plus-square">Create Playlist</i> </a>
      </li>
      <% allPlaylists.forEach(function(playlist) {%>
      <li>
        <a class="sidePlaylist btn btn-sm " href="/playlists/<%=playlist._id%>"> <img class="sideImage"
            src="<%= playlist.image%>" alt=""> <%= playlist.name %></a>
      </li>
      <%})%>
      <%}%>
    </ul>
  </div>
</div>